fun factorial(n) {
  if (n == 0) then { ret = 1 }
  else { ret = factorial(n - 1) * n };
    ${
    	IT'S SHOWTIME
    	  HEY CHRISTMAS TREE a
    	  YOU SET US UP 0.0
    	  HEY CHRISTMAS TREE b
    	  YOU SET US UP 17.4
    	  GET TO THE CHOPPER a
    	  HERE IS MY INVITATION 4.0
    	  GET UP b
    	  YOU'RE FIRED 2.0
    	  ENOUGH TALK
    	  TALK TO THE HAND a
    	YOU HAVE BEEN TERMINATED
    	}$;
  return ret
}

out(factorial(5))